<script>
    function DOMReady(e){"loading"!=document.readyState?e():document.addEventListener?document.addEventListener("DOMContentLoaded",e):document.attachEvent("onreadystatechange",function(){"loading"!=document.readyState&&e()})}
    function pegasus(a,b,c){return c=new XMLHttpRequest,c.open("GET",a),a=[],pegasus.timeout&&(c.timeout=pegasus.timeout),c.ontimeout=function(a){b=a},c.onreadystatechange=c.then=function(d,e,f,g){if(d&&d.call&&(a=[,d,e]),b&&a[2]&&a[2](b,c),4==c.readyState&&(f=a[0|c.status/200])){if(c.getResponseHeader("content-type").indexOf('application/json')!==-1){try{g=JSON.parse(c.responseText)}catch(a){g=null}}else{g=c.responseText}f(g,c)}},c.send(),c}


    function getUrlParameter(name) {
        name = name.replace(/[\[]/, '\\[').replace(/[\]]/, '\\]');
        var regex = new RegExp('[\\?&]' + name + '=([^&#]*)');
        var results = regex.exec(location.search);
        return results === null ? '' : decodeURIComponent(results[1].replace(/\+/g, ' '));
    
    }

function updatePIPModel(itemId) {
        var requestUrl = '/p/svcs/frontEndModel/' + itemId + '?_=' + Date.now();
        var storeSelectionQPVal = getUrlParameter('storeSelection');
        var storeQPVal = getUrlParameter('store');
        var tileToggleVal = getUrlParameter('tileToggle');
        var tileVersionVal = getUrlParameter('tileVersion');

        if (storeSelectionQPVal) {
            requestUrl += '&storeSelection=' + storeSelectionQPVal;
        
        }
        if (storeQPVal) {
          requestUrl += '&store=' + storeQPVal;
        
        }
        if (tileToggleVal) {
          requestUrl += '&tileToggle=' + tileToggleVal;
        
        }
        if (tileVersionVal) {
          requestUrl += '&tileVersion=' + tileVersionVal;
        
        }

        pegasus(requestUrl).then(function (pipModel) {
            resolvePipModelRequestPromise(pipModel);
        
            });
    
}

function insertPriceMarkup(priceFunc, priceSelector) {
  if (priceFunc) {
    priceFunc().then(function (response) {
        window.DOMReady(function(){
                    var price_container = document.querySelector(priceSelector);

                    if (price_container) {
                        var newDiv = document.createElement('div');
                        newDiv.innerHTML = response;

                        price_container.insertAdjacentElement('beforeend', newDiv);
                    
                    }
                
            });
            
        });
        
  }
    
}

function doPriceAndModelRequests(itemId) {

        insertPriceMarkup(window.getPrice, '.buybox__price');
        insertPriceMarkup(window.getReplacementModelPrice, '.discontinued__item-info__price');

        window.updatePIPModel(itemId);
    
}

function localizeAndLoadPriceAndLoadModelAndLoadPipBundle(itemId) {

  window.pipModelRequest = new Promise(function(resolve) {
      window.resolvePipModelRequestPromise = function(pipModel) {
                resolve(pipModel);
            
      }
        
      });

        var Localizer = window.THD_LOCALIZER_AUTO_INIT.Localizer;

        Localizer.on(Localizer.event.LOCALIZED_STORE_CHANGED, function() {
            var hasGpsWorkflow = Localizer.status.getWorkflow().indexOf('GPS') !== -1;

            if (hasGpsWorkflow) {
                window.location.reload();
            
            }
        
            });


        Localizer.localize().then(function() {
                doPriceAndModelRequests(itemId);
            
            });

    
}

function removeGStoreParamOnLoad(){
  try{
          var url = window.location.href;
          if(url.indexOf('g_store=')>-1){
              url = removeParamFromUrl(window.location.href, 'g_store');
              history.replaceState(null, null, url);
          
          }
      
  }catch(e){
          console.error(e);
      
  }
    
}

function removeParamFromUrl(url, paramName) {
  try{
            var paramIndex = url.indexOf(paramName);
            if(paramIndex>-1){
                var endIndex = url.indexOf('&', paramIndex)===-1 ? url.length : (url.indexOf('&', paramIndex + 1))
                var fullParam = url.substring(paramIndex, endIndex);
                url = url.replace(fullParam + '&', '');
                url = url.replace(fullParam, '');
                //remove trailing &/?
                var end = url.substring(url.length-1);
                if(end==='?' || end==='&'){
                    url = url.substring(0, url.length-1);
                
                }
            
            }
        
  }catch(e){
            console.error(e);
        
  }
        return url;
    
}

    localizeAndLoadPriceAndLoadModelAndLoadPipBundle('202970675')
    var Localizer = window.THD_LOCALIZER_AUTO_INIT.Localizer;
    Localizer.on(Localizer.event.LOCALIZED, function() {
        setTimeout(function() {
        removeGStoreParamOnLoad();
    
            }, 2000)
    
        })

</script>

<script>
function sendNewRelicEvent(actionName, errorObj) {
        if (window.newrelic
            && window.newrelic.addPageAction
            && typeof window.newrelic.addPageAction === 'function') {

            window.newrelic.addPageAction(actionName, errorObj);
            if (location.search.indexOf('debug') !== -1) {
                console.log('New Relic Event: ', actionName, errorObj);
            
            }
        
        }
    
}
setTimeout(function () {
        var buybeltErrorMessageEl = document.getElementById('buybelt__error-state');
        if (getComputedStyle(buybeltErrorMessageEl, null) && getComputedStyle(buybeltErrorMessageEl, null).display !== 'none') {
        sendNewRelicEvent('PIP_BUYBELT_ERROR_VISIBLE', {
                url: location.href
            
            })
        
        }
    
    }, 12000)
