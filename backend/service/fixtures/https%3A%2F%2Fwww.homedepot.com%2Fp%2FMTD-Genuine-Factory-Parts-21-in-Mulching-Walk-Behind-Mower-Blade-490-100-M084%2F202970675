    <div class="express-delivery-overlay__add-to-cart col-12 col-md-9 pip-row">
        {{#cart.visible}}
        <div class="add-to-cart__availability col-12 col-md-5">
            <span class="u__express-delivery_spacer"></span>
            <div class="{{#messaging.messageState}}{{messaging.messageState}}{{/messaging.messageState}}">
                {{#messaging.messageText}}

                <span class="{{#messaging.isBodfsAsServiceLevelEnabled}}u__bold{{/messaging.isBodfsAsServiceLevelEnabled}} availability__message-text">
                        {{{messaging.messageText}}}
                </span>
                {{/messaging.messageText}}
                {{#messaging.date}}
                    {{#messaging.isBodfsAsServiceLevelEnabled}}
                        <span class="u__block">Select date in checkout</span>
                    {{/messaging.isBodfsAsServiceLevelEnabled}}
                    {{^messaging.isBodfsAsServiceLevelEnabled}}
                        <span class="availability__date u__block u__bold">{{messaging.date}}</span>
                    {{/messaging.isBodfsAsServiceLevelEnabled}}
                {{/messaging.date}}
            </div>
        </div>
        <div class="add-to-cart__quantity col-12 col-md-4" id="express-delivery-overlay__quantity">
        </div>
        <div data-section="buybelt" class="col-12 col-md-3 u__align_right add-to-cart__button-wrapper">
            <span class="u__express-delivery_spacer"></span>
            <button data-target-atc="ATC" data-product-id="{{products.primary.data.itemId}}" id="express-delivery-ATC" class="add-to-cart__button bttn bttn--primary u__bold {{#cart.button.disabled}}disabled{{/cart.button.disabled}}" {{#cart.button.disabled}}disabled{{/cart.button.disabled}}>
                <span class="bttn__content">Add to Cart</span>
            </button>
        </div>
        {{/cart.visible}}
    </div>

</script>

