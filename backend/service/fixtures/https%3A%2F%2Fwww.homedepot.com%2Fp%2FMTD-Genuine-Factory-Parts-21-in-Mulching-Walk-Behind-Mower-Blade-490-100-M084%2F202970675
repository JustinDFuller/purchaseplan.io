              <span class="u__bold fbt__price">${{price}}</span>
            {{/mapAboveOriginalPrice}}
          {{/isPrimary}}
    {{/isMapPriceScenario}}

</script>
<script type="text/template" id="product_line">
    <div class="fbtProductLine">
        <div class="checkbox-btn">
            {{#itemId}}
              <input class="checkbox-btn__input" type="checkbox" value="value-1" id="fbt_{{itemId}}" name="fbt_{{itemId}}" checked="checked"  data-itemid="{{itemId}}" data-index="{{index}}" data-component="product pod">
            {{/itemId}}
              <input class="checkbox-btn__input" type="checkbox" value="value-1" id="fbt_{{productId}}" name="fbt_{{productId}}" checked="checked"  data-itemid="{{productId}}" data-index="{{index}}" data-component="product pod">
            <label class="checkbox-btn__label fbtProdDescription" for="fbt_{{#itemId}}{{itemId}}{{/itemId}}{{productId}}">
                <span class="fbtProductMsg">
                    {{#isPrimary}}<span class="u__bold">This item:</span> {{info.productLabel}} {{/isPrimary}}
                    {{^isPrimary}}
                        <a class="u__default-link" href="{{canonicalURL}}" data-index="{{index}}">
                          {{#info.brandName}} {{info.brandName}} {{/info.brandName}}
                          {{brand}}
                          {{#info.productLabel}} {{info.productLabel}} {{/info.productLabel}}
                          {{productName}}
                        </a>
                    {{/isPrimary}}

                    {{#storeSkus.0.itemPrice}}
                      {{>itemPrice}}
                    {{/storeSkus.0.itemPrice}}

                    {{^isPrimary}}
                      {{>itemPrice}}
                    {{/isPrimary}}
                </span>
            </label>
        </div>
    </div>
</script>
<script type="text/template" id="fbt_addToCart">
    {{#price.length}}
        {{^atcPriceLessthanDollar}}
            <span class="fbtAtcPrice__currency">&#036;</span>
            <span class="fbtAtcPrice__dollar">{{price.0}}</span>
            <span class="fbtAtcPrice__cents">{{price.1}}</span>
        {{/atcPriceLessthanDollar}}
        {{#atcPriceLessthanDollar}}
            <span class="fbtAtcPrice__dollar">{{price.1}}</span>
            <span class="fbtAtcPrice__centSign">&#162;</span>
        {{/atcPriceLessthanDollar}}
        {{#isSavingsPrice}}
            <div class="fbtAtcPrice__savings">
                <span class="u__strike">{{atcOriginalPrice}}</span>
