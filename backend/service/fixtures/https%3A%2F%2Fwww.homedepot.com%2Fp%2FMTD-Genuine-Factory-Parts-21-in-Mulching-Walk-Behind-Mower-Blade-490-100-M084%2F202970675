<script type="text/template" id="item_price">
    {{#isMapPriceScenario}}
        {{^mapAboveOriginalPrice}}
            {{#displayPriceLessThanDollar}}
                <span class="u__bold u__strike">{{formattedOriginalPrice}}</span><span class="u__strike u__bold fbt__cents">&#162;</span>
            {{/displayPriceLessThanDollar}}
