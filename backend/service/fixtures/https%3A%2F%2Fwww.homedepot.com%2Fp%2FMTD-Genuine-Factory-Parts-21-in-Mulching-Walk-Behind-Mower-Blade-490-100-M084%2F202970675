                {{#customer.localStore}}
                    at {{ customer.localStore.storeName  }}
                {{/customer.localStore}}
            {{/fulfillment.store.buyOnlinePickUpInStore}}
        </div>

        {{#fulfillment.store.buyOnlineShipToStore}}
