                <span class="u__bold u__strike">{{formattedOriginalPrice}}</span>
            {{/displayPriceLessThanDollar}}
        {{/mapAboveOriginalPrice}}
        <span>See Lower Price in Cart</span>
    {{/isMapPriceScenario}}
    {{^isMapPriceScenario}}
        {{#itemOnSale}}
            {{#originalPriceLessThanDollar}}
                <span class="u__strike fbt__price">{{formattedOriginalPrice}}</span><span class="u__strike fbt__cents">&#162;</span>
            {{/originalPriceLessThanDollar}}
            {{^originalPriceLessThanDollar}}
                <span class="u__strike fbt__price">{{formattedOriginalPrice}}</span>
            {{/originalPriceLessThanDollar}}
        {{/itemOnSale}}
        {{#displayPriceLessThanDollar}}
