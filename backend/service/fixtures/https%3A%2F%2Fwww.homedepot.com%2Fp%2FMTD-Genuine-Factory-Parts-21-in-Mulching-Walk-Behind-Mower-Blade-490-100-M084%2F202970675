                        <span>Not in stock at your</span>
                        <span class="warning_text">{{ customer.localStore.storeName  }}</span>
                        <span>store</span>
                    </div>
                    <div class="heading-message store-box__store-sku">
                        <span>Limited stock at </span>
                        <span class="warning_text">{{nearbyStore.name}}</span>
                        <span>({{nearbyStore.distance}} mi)</span>
                    </div>
                    <div>
                        <a href="#" class="u__default-link store-inventory-finder-trigger">Check Another Nearby Store</a>
                    </div>
                </div>
            {{/isLimitedQuantity}}
        {{/inventory.nearbyStore}}
    {{/features.multistoreFeatureEnabled}}
</script>

