{
  "expo": {
    "name": "Purchase Plan",
    "slug": "PurchasePlan",
    "version": "0.0.59",
    "orientation": "portrait",
    "icon": "./assets/adaptive-icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1d1d42"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.purchaseplan.ios",
      "buildNumber": "0.0.17"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.purchaseplan.android",
      "versionCode": 18,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1d1d42"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.purchaseplan.io",
              "pathPrefix": "/"
            },
            {
              "scheme": "http",
              "host": "*.purchaseplan.io",
              "pathPrefix": "/"
            },
            {
              "scheme": "https",
              "host": "purchaseplan.io",
              "pathPrefix": "/"
            },
            {
              "scheme": "http",
              "host": "purchaseplan.io",
              "pathPrefix": "/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.ico"
    }
  }
}
